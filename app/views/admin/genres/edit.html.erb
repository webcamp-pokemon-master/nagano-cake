<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h2 style="background:#EEEEEE;">ジャンル一覧・追加</h2>
        <%= form_with model: @genre, url: "/admin/genres/#{@genre.id}",local:true do |f| %>
          ジャンル名
          <%= f.text_field :name %>
          <%= f.submit "変更を保存", class: "btn btn-success rounded-pill" %>
        <% end %>
    </div>
  </div>
</div>
